      </None>
      <None Update="CacheFiles\model.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Update="CacheFiles\binary.data">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Update="CacheFiles\binary_read_only.data">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Update="CacheFiles\newtonsoft_read_only.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
      <None Update="CacheFiles\protobuf_read_only.bin">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </None>
    </ItemGroup>

</Project>
