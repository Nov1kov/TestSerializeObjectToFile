<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>

        <IsPackable>false</IsPackable>

        <AssemblyName>TestSerializeObjectToFile</AssemblyName>

        <RootNamespace>TestSerializeObjectToFile</RootNamespace>
    </PropertyGroup>

